// retrieve node in DOM via ID
var c = document.getElementById( "slate");
console.log( 'hi');

// instantiate a CanvaRenderingContext2D object
var ctx = c.getContext( "2d");

// invoke interface methods
ctx.fillStyle = "#ff0000";
ctx.fillRect( 50, 50, 100, 200);

